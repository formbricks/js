{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "@formbricks/js:test": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "playground:build": {
      "dependsOn": ["@formbricks/js:build"],
      "outputs": ["apps/playground/out/**"],
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "test:coverage": {
      "outputs": []
    }
  }
}
